{
  "name": "@winstonqian7/component-lib",
  "version": "0.1.0",
  "private": false,
  "dependencies": {
    "@types/node": "^16.11.48",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "styled-components": "5.3.5",
    "typescript": "^4.7.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "tsc",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/node-logger": "^6.5.10",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.0.3",
    "@types/jest-axe": "^3.5.4",
    "@types/styled-components": "5.1.26",
    "babel-plugin-named-exports-order": "^0.0.2",
    "jest": "29.0.3",
    "jest-axe": "5.0.1",
    "prop-types": "^15.8.1",
    "semantic-release": "19.0.5",
    "webpack": "^5.74.0"
  },
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    "@semantic-release/github",
    "@semantic-release/npm",
    "@semantic-release/git"
  ],
  "description": "Components Lib using React-Typescript, storybook, and styled components for use on coding projects",
  "main": "index.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WinstonQian7/UI-Component-Lib.git"
  },
  "author": "Winston Qian",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/WinstonQian7/UI-Component-Lib/issues"
  },
  "homepage": "https://github.com/WinstonQian7/UI-Component-Lib#readme"
}
